From: Andreas Schneider <asn@cryptomilk.org>
Date: Fri, 13 Mar 2015 09:01:03 +0000
Subject: sepolicy: fscklogs are covered upstream now

---
diff --git a/BoardConfig.mk b/BoardConfig.mk
index 47cbcdb..9bd90ff 100644
--- a/BoardConfig.mk
+++ b/BoardConfig.mk
@@ -121,11 +121,9 @@ BOARD_SEPOLICY_DIRS := \
 
 BOARD_SEPOLICY_UNION := \
 	app.te \
-	device.te \
 	domain.te \
 	file_contexts \
 	file.te \
-	vold.te \
 	ueventd.te
 
 ###########################################################
diff --git a/sepolicy/device.te b/sepolicy/device.te
deleted file mode 100644
index 3bf5c69..0000000
--- a/sepolicy/device.te
+++ /dev/null
@@ -1 +0,0 @@
-type fscklogs, dev_type;
diff --git a/sepolicy/file_contexts b/sepolicy/file_contexts
index 107ccfb..58312ee 100644
--- a/sepolicy/file_contexts
+++ b/sepolicy/file_contexts
@@ -11,8 +11,6 @@
 /dev/v4l-subdev[0-9]*   u:object_r:video_device:s0
 /dev/media[0-3]*        u:object_r:camera_device:s0
 
-/dev/fscklogs(/.*)?     u:object_r:fscklogs:s0
-
 ####################################
 # wifi files
 /data/\.cid\.info       u:object_r:wifi_data_file:s0
diff --git a/sepolicy/vold.te b/sepolicy/vold.te
deleted file mode 100644
index d7eeed0..0000000
--- a/sepolicy/vold.te
+++ /dev/null
@@ -1,3 +0,0 @@
-# Log fsck results
-allow vold fscklogs:dir rw_dir_perms;
-allow vold fscklogs:file create_file_perms;
